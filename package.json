{
	"name": "deedee",
	"version": "1.4.37",
	"description": "A tool for detecting dependencies",
	"license": "MIT",
	"repository": "https://github.com/fdesjardins/deedee",
	"homepage": "https://github.com/fdesjardins/deedee",
	"keywords": [
		"detect",
		"dependencies",
		"libraries",
		"node",
		"bower",
		"php",
		"composer",
		"development",
		"libs",
		"deps",
		"resources",
		"detection",
		"projects"
	],
	"main": "lib/deedee.js",
	"bin": "lib/deedee-cli.js",
	"scripts": {
		"build": "babel src/ -d lib/",
		"precoverage": "npm run build",
		"coverage": "babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
		"precoveralls": "npm run coverage",
		"coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
		"prepublish": "npm run build",
		"pretest": "npm run build",
		"test": "mocha --require babel-register"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-istanbul": "^0.12.2",
		"babel-plugin-rewire": "^1.2.0",
		"babel-plugin-syntax-async-functions": "^6.13.0",
		"babel-plugin-transform-regenerator": "^6.26.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"chai": "^4.2.0",
		"child-process-promise": "^2.2.1",
		"coveralls": "^3.0.3",
		"eslint": "^5.14.1",
		"eslint-config-xo": "^0.26.0",
		"istanbul": "^1.0.0-alpha.2",
		"mocha": "^6.0.1",
		"mocha-lcov-reporter": "^1.3.0",
		"sinon": "^7.2.4",
		"strip-ansi": "^5.0.0"
	},
	"dependencies": {
		"bluebird": "^3.5.3",
		"chalk": "^2.4.2",
		"lodash": "^4.17.11",
		"text-table": "^0.2.0",
		"walk": "^2.3.14",
		"yargs": "^13.2.1"
	}
}
