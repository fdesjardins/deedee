{
  "name": "deedee",
  "version": "1.0.4",
  "description": "A tool for detecting project dependencies.",
  "license": "MIT",
  "repository": "https://github.com/fdesjardins/deedee",
  "homepage": "https://github.com/fdesjardins/deedee",
  "keywords": [
    "detect",
    "dependencies",
    "libraries",
    "node",
    "bower"
  ],
  "main": "lib/deedee.js",
  "bin": "lib/deedee-cli.js",
  "scripts": {
    "build": "babel src/ -d lib/",
    "precoverage": "npm run build",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "precoveralls": "npm run coverage",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-istanbul": "^0.7.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "child-process-promise": "^1.1.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.4.0",
    "eslint-config-xo": "^0.12.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "chalk": "^1.1.1",
    "lodash": "^4.6.1",
    "text-table": "^0.2.0",
    "walk": "^2.3.9",
    "yargs": "^4.3.1"
  }
}
