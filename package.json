{
  "name": "deedee",
  "version": "1.4.12",
  "description": "A tool for detecting dependencies",
  "license": "MIT",
  "repository": "https://github.com/fdesjardins/deedee",
  "homepage": "https://github.com/fdesjardins/deedee",
  "keywords": [
    "detect",
    "dependencies",
    "libraries",
    "node",
    "bower",
    "php",
    "composer",
    "development",
    "libs",
    "deps",
    "resources",
    "detection",
    "projects"
  ],
  "main": "lib/deedee.js",
  "bin": "lib/deedee-cli.js",
  "scripts": {
    "build": "babel src/ -d lib/",
    "precoverage": "npm run build",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "precoveralls": "npm run coverage",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "chai": "^3.5.0",
    "child-process-promise": "^2.2.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-xo": "^0.18.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^2.1.0",
    "strip-ansi": "^3.0.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^1.1.3",
    "lodash": "^4.17.4",
    "text-table": "^0.2.0",
    "walk": "^2.3.9",
    "yargs": "^7.0.2"
  }
}
