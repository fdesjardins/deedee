{
  "name": "deedee",
  "version": "1.4.32",
  "description": "A tool for detecting dependencies",
  "license": "MIT",
  "repository": "https://github.com/fdesjardins/deedee",
  "homepage": "https://github.com/fdesjardins/deedee",
  "keywords": [
    "detect",
    "dependencies",
    "libraries",
    "node",
    "bower",
    "php",
    "composer",
    "development",
    "libs",
    "deps",
    "resources",
    "detection",
    "projects"
  ],
  "main": "lib/deedee.js",
  "bin": "lib/deedee-cli.js",
  "scripts": {
    "build": "babel src/ -d lib/",
    "precoverage": "npm run build",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "precoveralls": "npm run coverage",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "eslint-config-xo": "^0.24.2",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^6.1.5",
    "strip-ansi": "^4.0.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chalk": "^2.4.1",
    "lodash": "^4.17.10",
    "text-table": "^0.2.0",
    "walk": "^2.3.14",
    "yargs": "^12.0.1"
  }
}
